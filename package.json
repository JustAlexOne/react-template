{
  "name": "app",
  "version": "1.0.0",
  "description": "React application template",
  "private": true,
  "main": "lib/main.js",
  "jsnext:main": "lib/main.js",
  "scripts": {
    "lint": "eslint src tests",
    "test": "NODE_ENV=test mocha --compilers js:babel-core/register tests/index.js",
    "test:watch": "npm test -- --watch",
    "start": "NODE_ENV=dev webpack-dev-server -d --colors --hot --inline --progress",
    "bundle": "NODE_ENV=production webpack -p --colors --progress",
    "lib": "rm -rf ./lib && babel src --out-dir lib",
    "build": "npm run test && npm run lint && npm run lib && npm run bundle"
  },
  "author": "Mathieu ANCELIN",
  "license": "MIT",
  "dependencies": {
    "babel-polyfill": "^6.3.14",
    "moment": "^2.10.6",
    "react": "^0.14.3",
    "react-dom": "^0.14.3"
  },
  "devDependencies": {
    "babel": "^6.3.13",
    "babel-cli": "^6.3.17",
    "babel-core": "^6.3.21",
    "babel-eslint": "^5.0.0-beta6",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "chai": "^3.4.1",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^2.1.1",
    "eslint-plugin-react": "^3.11.3",
    "jsdom": "^7.2.1",
    "mocha": "^2.3.4",
    "react-addons-test-utils": "^0.14.3",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0"
  }
}
