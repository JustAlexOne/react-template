  },
  "devDependencies": {
    "babel": "^5.8.29",
    "babel-core": "^5.8.29",
    "babel-eslint": "^4.1.3",
    "babel-loader": "^5.3.2",
    "chai": "^3.4.0",
    "eslint": "^1.8.0",
    "eslint-config-airbnb": "^0.1.0",
    "eslint-plugin-react": "^3.6.3",
    "jsdom": "^7.0.2",
    "mocha": "^2.3.3",
    "react-addons-test-utils": "^0.14.1",
    "webpack": "^1.12.2",
