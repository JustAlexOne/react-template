{
  "name": "app",
  "version": "1.0.0",
  "description": "React application template",
  "private": true,
  "main": "lib/main.js",
  "jsnext:main": "lib/main.js",
  "scripts": {
    "lint": "eslint src tests",
    "test": "NODE_ENV=test mocha --compilers js:babel-core/register tests/index.js",
    "start": "NODE_ENV=dev webpack-dev-server -d --colors --hot --inline --progress",
    "bundle": "NODE_ENV=production webpack -p --colors --progress",
    "lib": "babel src --out-dir lib",
    "build": "npm run test && npm run lint && npm run lib && npm run bundle"
  },
  "author": "Mathieu ANCELIN",
  "license": "MIT",
  "dependencies": {
    "babel-polyfill": "^6.2.0",
    "moment": "^2.10.6",
    "react": "^0.14.3",
    "react-dom": "^0.14.3"
  },
  "devDependencies": {
    "babel": "^6.1.18",
    "babel-cli": "^6.2.0",
    "babel-core": "^6.2.1",
    "babel-eslint": "^5.0.0-beta4",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "babel-preset-stage-0": "^6.1.18",
    "chai": "^3.4.1",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^1.0.2",
    "eslint-plugin-react": "^3.11.2",
    "jsdom": "^7.1.0",
    "mocha": "^2.3.4",
    "react-addons-test-utils": "^0.14.3",
    "react-hot-loader": "^1.3.0",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0"
  }
}
